
<div class="wrap">
	<table class="admin-table">
		<thead>
			<tr>
				<th><%= sortable("id", "ID") %></th>
				<th><%= sortable("email", "Email") %></th>
				<th><%= sortable("created_at", "Created At") %></th>
				<th><%= sortable("sign_in_count", "Sign In Count") %></th>
				<th><%= sortable("current_sign_in", "Current Sign In") %></th>
				<th>Actions</th>
			</tr>
		</thead>
		<tbody>
			<% for user in @users %>
			<tr>
				<td><%= user.id %></td>
				<td><%= user.email %></td>
				<td><%= user.created_at %></td>
				<td><%= user.sign_in_count %></td>
				<td><%= user.current_sign_in_at %></td>
				<td><%= link_to "Edit", edit_admin_user_path(user) %> | <%= link_to "View", admin_user_path(user) %></td>
			</tr>
			<% end %>
		</tbody>
	</table>
</div>