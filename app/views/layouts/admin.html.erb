<!DOCTYPE html>
<% if content_for?(:html) %>
	<%= yield(:html) %>
<% else %>
	<html>
<% end %>
<head>
<title><%= yield(:title) %></title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>

  <%= csrf_meta_tags %>

  <%= yield :head %>

<!-- Mobile browser viewport -->
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
</head>
<body>
<%= render "admin/nav" %>

<%= yield %>


</body>
</html>
